	#nntab ul.tabs li a { display:block; width:168px; height:40px; text-align:center; line-height:40px; font-size:12px; text-decoration:none; position:relative; top:0px; padding:0; border: 1px solid #ddd; border-bottom-color:#aaa;color: #7d7d7d; background:#f6f6f6; }
